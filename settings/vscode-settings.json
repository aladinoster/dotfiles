{
    // General Settings VS Code
    // Window
    "window.newWindowDimensions": "fullscreen",
    "window.restoreFullscreen": true,
    "window.closeWhenEmpty": true,
    // File management
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1500,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.hotExit": "onExitAndWindowClose",
    //Explorer
    "explorer.openEditors.visible": 0,
    "workbench.statusBar.visible": true,
    "workbench.editor.closeEmptyGroups": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.settings.editor": "json",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 8,
    "workbench.colorCustomizations": {
        "contrastActiveBorder": "#fffb0050",
        "inputOption.activeBackground": "#3375eebd",
        "activityBar.activeBackground": "#295c8a",
        "debugConsole.sourceForeground": "#ffffff",
        "[Material Theme Palenight High Contrast]": {}
    },
    "workbench.editor.tabSizing": "shrink",
    "workbench.panel.defaultLocation": "bottom",
    "workbench.list.openMode": "singleClick",
    // Local history
    "workbench.localHistory.enabled": true,
    // Editor settings
    "editor.autoIndent": "full",
    "editor.fontLigatures": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "expand",
    "editor.cursorWidth": 3,
    "editor.tabSize": 4,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "editor.rulers": [
        120,
    ],
    "editor.formatOnSave": true,
    "editor.suggestFontSize": 14,
    "editor.fontSize": 14,
    "editor.fontFamily": "Fira Code, Monaco",
    "editor.fontWeight": "400",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "selection",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderFinalNewline": "on",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.largeFileOptimizations": false,
    "editor.lineNumbers": "relative",
    // Zen Mode
    "zenMode.fullScreen": false,
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": true,
    "zenMode.silentNotifications": true,
    // Debugging
    "debug.console.fontFamily": "default",
    "debug.console.fontSize": 14,
    "debug.toolBarLocation": "docked",
    // Terminal
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "terminal.integrated.fontWeightBold": "200",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.showExitAlert": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.rendererType": "auto",
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.minimumContrastRatio": 4.5,
    // scm
    "scm.alwaysShowRepositories": true,
    // Explorer
    "explorer.confirmDelete": false,
    "explorer.autoReveal": false,
    "explorer.decorations.badges": false,
    "breadcrumbs.enabled": true,
    "explorer.compactFolders": true,
    "breadcrumbs.filePath": "on",
    "breadcrumbs.symbolPath": "on",
    "breadcrumbs.symbolSortOrder": "type",
    "[python]": {
        "editor.wordWrapColumn": 80,
        "editor.rulers": [
            80,
        ],
        "editor.formatOnType": true,
    },
    // ssh
    // "remote.SSH.useLocalServer": true,
    // "remote.SSH.lockfilesInTmp": true,
    // "remote.SSH.useFlock":false,
    // latex
    "[latex]": {
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.suggestSelection": "recentlyUsedByPrefix",
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        },
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "[jupyter]": {
        "editor.colorDecorators": true,
    },
    // Git
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.autoStash": true,
    "git.showPushSuccessNotification": true,
    // Problems
    "problems.decorations.enabled": true,
    // Extensions
    // CMake
    "cmake.configureOnOpen": false,
    // Code Runner
    "code-runner.executorMap": {
        "javascript": "node",
        "python": "$pythonPath -u $fullFileName",
    },
    "code-runner.showExecutionMessage": false,
    "code-runner.clearPreviousOutput": true,
    // C++
    "C_Cpp.updateChannel": "Insiders",
    // CSpell
    "cSpell.enableFiletypes": [
        "latex",
        "markdown",
        "!python"
    ],
    "cSpell.userWords": [
        "biber",
        "pdflatex"
    ],
    // Git Lens
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
    },
    // Julia
    // LaTeX
    "latex-workshop.latex.tools": [
        {
            "command": "lualatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ],
            "name": "Step 1: lualatex"
        },
        {
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "name": "Step 2: bibtex"
        },
        {
            "command": "lualatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "name": "Step 3: lualatex"
        },
        {
            "command": "lualatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "name": "Step 4: lualatex"
        },
        {
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ],
            "name": "Step 1: pdflatex"
        },
        {
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "name": "Step 3: pdflatex"
        },
        {
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "name": "Step 4: pdflatex"
        },
        {
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ],
            "name": "Step 2: biber"
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "[1] pdflatex",
            "tools": [
                "Step 1: pdflatex",
            ]
        },
        {
            "name": "[2] lualatex",
            "tools": [
                "Step 1: lualatex",
            ]
        },
        {
            "name": "[1a] pdflatex->biber->pdflatex*2",
            "tools": [
                "Step 1: pdflatex",
                "Step 2: biber",
                "Step 3: pdflatex",
                "Step 4: pdflatex"
            ]
        },
        {
            "name": "[2a] lualatex->biber->lualatex*2",
            "tools": [
                "Step 1: lualatex",
                "Step 2: biber",
                "Step 3: lualatex",
                "Step 4: lualatex"
            ]
        },
        {
            "name": "[1b] lualatex->bibtex->lualatex*2",
            "tools": [
                "Step 1: lualatex",
                "Step 2: bibtex",
                "Step 3: lualatex",
                "Step 4: lualatex"
            ]
        },
        {
            "name": "[2b] pdflatex->bibtex->pdflatex*2",
            "tools": [
                "Step 1: pdflatex",
                "Step 2: bibtex",
                "Step 3: pdflatex",
                "Step 4: pdflatex"
            ]
        },
    ],
    "latex-workshop.view.pdf.hand": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
    // Python
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length",
        "80"
    ],
    "python.pythonPath": "/Users/andresladino/anaconda3/bin/python",
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [],
    "python.terminal.executeInFileDir": false,
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "basic",
    // AutoDocstring
    "autoDocstring.docstringFormat": "google",
    // TodoTree
    "todo-tree.general.tags": [
        "TODO:",
        "FIXME:",
        "REVIEW:",
        "NOTE:",
        "[ ]",
        "[x]"
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO:": {
            "icon": "checklist",
            "foreground": "#045de4",
            "type": "line"
        },
        "FIXME:": {
            "icon": "issue-opened",
            "foreground": "#ec80f0c4",
            "type": "line"
        },
        "REVIEW:": {
            "icon": "eye",
            "foreground": "green",
            "type": "line"
        },
        "NOTE:": {
            "icon": "note",
            "foreground": "orange",
            "type": "line"
        }
    },
    "todo-tree.tree.showCountsInTree": true,
    // XML
    "xmlTools.xmlFormatterImplementation": "v2",
    "git.confirmSync": false,
    "http.proxySupport": "off",
    "jupyter.askForKernelRestart": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "julia.enableTelemetry": true,
    "python.defaultInterpreterPath": "/Users/andresladino/opt/anaconda3/bin/python",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "redhat.telemetry.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "stash-push:command",
        "switch:command",
        "pull:command"
    ],
    "julia.symbolCacheDownload": true,
    "editor.accessibilitySupport": "off",
    "diffEditor.ignoreTrimWhitespace": false,
    "githubPullRequests.fileListLayout": "flat",
    "tabnine.experimentalAutoImports": true,
    "workbench.colorTheme": "Community Material Theme Palenight High Contrast",
    "window.zoomLevel": -1,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "vsicons.dontShowNewVersionMessage": true,
    "zenMode.showTabs": "single",
    "workbench.activityBar.location": "top",
    "vim.enableNeovim": true,
    "vim.neovimUseConfigFile": true
}